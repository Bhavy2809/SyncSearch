# Environment
NODE_ENV=development

# Database
DATABASE_URL=postgresql://syncsearch:devpassword@localhost:5432/syncsearch

# RabbitMQ
RABBITMQ_URL=amqp://syncsearch:devpassword@localhost:5672
QUEUE_NAME=media.extract_audio
PREFETCH_COUNT=5

# S3 Storage
S3_ENDPOINT=http://localhost:9000
S3_BUCKET=syncsearch-media
S3_ACCESS_KEY=minioadmin
S3_SECRET_KEY=minioadmin
S3_REGION=us-east-1
S3_USE_PATH_STYLE=true

# Processing
MAX_FILE_SIZE_GB=5
TEMP_DIR=/tmp/syncsearch
AUDIO_BITRATE=192k
